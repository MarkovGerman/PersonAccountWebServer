@model PersonalAccountWebServer.Models.PersonalAccount

@{
    ViewData["Title"] = "Accounts Delete";
}

<h1>Удалить</h1>

<h3>Вы действительно хотите удалить ЛС?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            Номер ЛС
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumberPA)
        </dd>
        <dt class="col-sm-2">
            Дата старта
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateStart)
        </dd>
        <dt class="col-sm-2">
            Дата окончания
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEnd)
        </dd>
        <dt class="col-sm-2">
            Адрес
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class="col-sm-2">
            Площадь
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Area)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id"/>
        <input type="submit" value="Удалить" class="btn btn-danger"/> |
        <a asp-action="Index">Вернуться</a>
    </form>
</div>